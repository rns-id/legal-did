{
  "name": "@rnsdid-protocol/rnsdid",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "tooling/*"
  ],
  "scripts": {
    "clean": "turbo run clean; rm -rf node_modules yarn.lock",
    "build": "turbo run build",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "clippy": "cargo clippy --all-targets -- -D warnings -A clippy::result_large_err -A clippy::too_many_arguments",
    "evm:compile": "hardhat compile",
    "evm:test": "hardhat test tests/evm/**/*.ts",
    "evm:test:legal-attestation": "hardhat test tests/evm/legal-attestation/TaggedAttester.test.ts tests/evm/legal-attestation/TaggedResolver.test.ts tests/evm/legal-attestation/TaggedQuery.test.ts tests/evm/legal-attestation/TaggedSchemaRegistrar.test.ts tests/evm/legal-attestation/Integration.test.ts",
    "evm:test:legal-attestation:attester": "hardhat test tests/evm/legal-attestation/TaggedAttester.test.ts",
    "evm:test:legal-attestation:resolver": "hardhat test tests/evm/legal-attestation/TaggedResolver.test.ts",
    "evm:test:legal-attestation:query": "hardhat test tests/evm/legal-attestation/TaggedQuery.test.ts",
    "evm:test:legal-attestation:registrar": "hardhat test tests/evm/legal-attestation/TaggedSchemaRegistrar.test.ts",
    "evm:test:legal-attestation:integration": "hardhat test tests/evm/legal-attestation/Integration.test.ts",
    "evm:test:legal-did": "hardhat test tests/evm/legal-did/LegalDID.test.ts tests/evm/legal-did/gas-analysis.test.ts",
    "evm:test:legal-did:main": "hardhat test tests/evm/legal-did/LegalDID.test.ts",
    "evm:test:legal-did:gas": "hardhat test tests/evm/legal-did/gas-analysis.test.ts",
    "evm:test:coverage": "hardhat coverage",
    "evm:test:coverage:legal-attestation": "hardhat coverage --testfiles \"tests/evm/legal-attestation/**/*.test.ts\"",
    "evm:test:runner": "ts-node scripts/evm/run-legal-attestation-tests.ts",
    "evm:test:runner:coverage": "ts-node scripts/evm/run-legal-attestation-tests.ts --coverage",
    "evm:test:runner:gas": "ts-node scripts/evm/run-legal-attestation-tests.ts --gas",
    "evm:test:runner:full": "ts-node scripts/evm/run-legal-attestation-tests.ts --coverage --gas",
    "evm:test:all": "ts-node scripts/evm/run-all-tests.ts",
    "evm:test:all:coverage": "ts-node scripts/evm/run-all-tests.ts --coverage",
    "evm:test:all:gas": "ts-node scripts/evm/run-all-tests.ts --gas",
    "evm:test:all:full": "ts-node scripts/evm/run-all-tests.ts --coverage --gas",
    "evm:test-compilation": "hardhat run scripts/evm/test-compilation.ts",
    "evm:verify-paths": "./scripts/evm/simple-path-check.sh",
    "evm:deploy:local": "hardhat run scripts/evm/deploy.ts --network localhost",
    "evm:deploy:sepolia": "hardhat run scripts/evm/deploy.ts --network sepolia",
    "evm:deploy:all:local": "hardhat run scripts/evm/deploy-all-contracts.ts --network localhost",
    "evm:deploy:all:sepolia": "hardhat run scripts/evm/deploy-all-contracts.ts --network sepolia",
    "evm:deploy:all:mainnet": "hardhat run scripts/evm/deploy-all-contracts.ts --network mainnet",
    "evm:node": "hardhat node",
    "svm:build": "anchor build",
    "svm:test": "anchor test",
    "svm:deploy:devnet": "anchor deploy --provider.cluster devnet",
    "svm:init-devnet": "ts-node scripts/svm/init-devnet.ts",
    "svm:mint-devnet": "ts-node scripts/svm/mint-devnet.ts",
    "svm:admin-revoke": "ts-node scripts/svm/admin-revoke.ts",
    "svm:user-burn": "ts-node scripts/svm/user-burn.ts",
    "svm:update-base-uri": "ts-node scripts/svm/update-base-uri.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.31.1",
    "@coral-xyz/borsh": "^0.2.6",
    "@dankelleher/solana-attestation-service-client": "^0.1.0",
    "@ethereum-attestation-service/eas-contracts": "^1.8.0",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^0.8.7",
    "@metaplex-foundation/umi-bundle-defaults": "^0.8.7",
    "@openzeppelin/contracts": "^5.4.0",
    "@solana/kit": "^5.1.0",
    "@solana/signers": "^5.1.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.95.0",
    "arweave": "^1.15.1",
    "bs58": "^5.0.0",
    "dotenv": "^16.0.3",
    "sas-lib": "^1.0.10",
    "turbo": "^1.10.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@metaplex-foundation/kinobi": "^0.12.3",
    "@metaplex-foundation/shank-js": "^0.1.5",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-upgrades": "^3.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.10.0",
    "chai": "^4.3.4",
    "ethers": "^6.13.0",
    "hardhat": "^2.22.0",
    "hardhat-contract-sizer": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.8",
    "mocha": "^10.7.0",
    "prettier": "^2.6.2",
    "solidity-coverage": "^0.8.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.5",
    "typechain": "^8.3.0",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@1.22.17",
  "resolutions": {
    "axios": ">=1.7.0",
    "form-data": ">=4.0.4",
    "js-yaml": ">=4.1.1",
    "nanoid": ">=3.3.8",
    "serialize-javascript": ">=6.0.2",
    "cookie": ">=0.7.0",
    "tmp": ">=0.2.4",
    "esbuild": ">=0.25.0",
    "bigint-buffer": "npm:@trufflesuite/bigint-buffer@^1.1.10"
  }
}
