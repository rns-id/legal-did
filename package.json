{
  "name": "@rnsdid-protocol/rnsdid",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "tooling/*"
  ],
  "scripts": {
    "clean": "turbo run clean; rm -rf node_modules yarn.lock",
    "build": "turbo run build",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "clippy": "cargo clippy --all-targets -- -D warnings -A clippy::result_large_err -A clippy::too_many_arguments",
    "evm:compile": "hardhat compile",
    "evm:test": "hardhat test tests/evm/**/*.ts",
    "evm:deploy:local": "hardhat run scripts/evm/deploy.ts --network localhost",
    "evm:deploy:sepolia": "hardhat run scripts/evm/deploy.ts --network sepolia",
    "evm:node": "hardhat node",
    "svm:build": "anchor build",
    "svm:test": "anchor test",
    "svm:deploy:devnet": "anchor deploy --provider.cluster devnet",
    "svm:init-devnet": "ts-node scripts/svm/init-devnet.ts",
    "svm:mint-devnet": "ts-node scripts/svm/mint-devnet.ts",
    "svm:admin-revoke": "ts-node scripts/svm/admin-revoke.ts",
    "svm:user-burn": "ts-node scripts/svm/user-burn.ts",
    "svm:update-base-uri": "ts-node scripts/svm/update-base-uri.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.31.1",
    "@coral-xyz/borsh": "^0.2.6",
    "@dankelleher/solana-attestation-service-client": "^0.1.0",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@metaplex-foundation/umi": "^0.8.7",
    "@metaplex-foundation/umi-bundle-defaults": "^0.8.7",
    "@solana/kit": "^5.1.0",
    "@solana/signers": "^5.1.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.95.0",
    "arweave": "^1.15.1",
    "bs58": "^5.0.0",
    "dotenv": "^16.0.3",
    "sas-lib": "^1.0.10",
    "turbo": "^1.10.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@metaplex-foundation/kinobi": "^0.12.3",
    "@metaplex-foundation/shank-js": "^0.1.5",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-upgrades": "^3.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.10.0",
    "chai": "^4.3.4",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.8",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "solidity-coverage": "^0.8.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typechain": "^8.3.0",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@1.22.17"
}
